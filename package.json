{
    "name": "qbs-tools",
    "displayName": "QBS",
    "description": "QBS support in Visual Studio Code",
    "version": "1.0.6",
    "publisher": "qbs-community",
    "author": {
        "name": "Denis Shienkov"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/denis-shienkov/vscode-qbs"
    },
    "bugs": {
        "url": "https://github.com/denis-shienkov/vscode-qbs/issues"
    },
    "homepage": "https://github.com/denis-shienkov/vscode-qbs",
    "keywords": [
        "qbs",
        "tools",
        "build",
        "c++",
        "native"
    ],
    "engines": {
        "vscode": "^1.73.1"
    },
    "categories": [
        "Other"
    ],
    "icon": "res/logo.png",
    "activationEvents": [
        "workspaceContains:*.qbs"
    ],
    "main": "./out/qbsextension.js",
    "contributes": {
        "languages": [
            {
                "id": "qbs",
                "extensions": [
                    ".qbs"
                ],
                "configuration": "./qbs-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "qbs",
                "scopeName": "source.qbs",
                "path": "./syntaxes/qbs.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "qbs.openTextDocumentAtPosition",
                "title": "%qbs-tools.command.qbs.open.text.document.pos.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.detectProfiles",
                "title": "%qbs-tools.command.qbs.detect.profiles.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.restoreProject",
                "title": "%qbs-tools.command.qbs.restore.project.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.autoRestartSession",
                "title": "%qbs-tools.command.qbs.autorestart.session.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.startSession",
                "title": "%qbs-tools.command.qbs.start.session.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.stopSession",
                "title": "%qbs-tools.command.qbs.stop.session.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.selectProject",
                "title": "%qbs-tools.command.qbs.select.project.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.selectProfile",
                "title": "%qbs-tools.command.qbs.select.profile.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.selectConfiguration",
                "title": "%qbs-tools.command.qbs.select.configuration.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.selectBuild",
                "title": "%qbs-tools.command.qbs.select.build.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.selectRun",
                "title": "%qbs-tools.command.qbs.select.run.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.selectDebugger",
                "title": "%qbs-tools.command.qbs.select.debugger.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.resolveWithForceProbesExecution",
                "title": "%qbs-tools.command.qbs.resolveforceprobes.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.resolve",
                "title": "%qbs-tools.command.qbs.resolve.title%",
                "icon": {
                    "dark": "res/dark/resolve-icon.svg",
                    "light": "res/light/resolve-icon.svg"
                },
                "category": "QBS"
            },
            {
                "command": "qbs.build",
                "title": "%qbs-tools.command.qbs.build.title%",
                "icon": {
                    "dark": "res/dark/build-icon.svg",
                    "light": "res/light/build-icon.svg"
                },
                "category": "QBS"
            },
            {
                "command": "qbs.clean",
                "title": "%qbs-tools.command.qbs.clean.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.install",
                "title": "%qbs-tools.command.qbs.install.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.rebuild",
                "title": "%qbs-tools.command.qbs.rebuild.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.cancel",
                "title": "%qbs-tools.command.qbs.cancel.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.getRunEnvironment",
                "title": "%qbs-tools.command.qbs.getrunenv.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.run",
                "title": "%qbs-tools.command.qbs.run.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.debug",
                "title": "%qbs-tools.command.qbs.debug.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.buildProduct",
                "title": "%qbs-tools.command.qbs.build.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.cleanProduct",
                "title": "%qbs-tools.command.qbs.clean.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.buildSubProject",
                "title": "%qbs-tools.command.qbs.build.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.cleanSubProject",
                "title": "%qbs-tools.command.qbs.clean.title%",
                "category": "QBS"
            },
            {
                "command": "qbs.editConfiguration",
                "title": "%qbs-tools.command.qbs.edit.configuration.title%",
                "category": "QBS"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "qbs.openTextDocumentAtPosition",
                    "when": "never"
                },
                {
                    "command": "qbs.buildProduct",
                    "when": "never"
                },
                {
                    "command": "qbs.cleanProduct",
                    "when": "never"
                },
                {
                    "command": "qbs.buildSubProject",
                    "when": "never"
                },
                {
                    "command": "qbs.cleanSubProject",
                    "when": "never"
                },
                {
                    "command": "qbs.getRunEnvironment",
                    "when": "never"
                },
                {
                    "command": "qbs.detectProfiles",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.restoreProject",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.autoRestartSession",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.startSession",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.stopSession",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.selectProject",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.selectProfile",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.selectConfiguration",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.selectBuild",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.selectRun",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.selectDebugger",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.resolveWithForceProbesExecution",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.resolve",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.build",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.clean",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.install",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.rebuild",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.cancel",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.run",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.debug",
                    "when": "qbs:extension-activated"
                },
                {
                    "command": "qbs.editConfiguration",
                    "when": "qbs:extension-activated"
                }
            ],
            "view/title": [
                {
                    "command": "qbs.resolve",
                    "when": "view == qbs-project && qbs:extension-activated",
                    "group": "navigation@1"
                },
                {
                    "command": "qbs.build",
                    "when": "view == qbs-project && qbs:extension-activated",
                    "group": "navigation@2"
                },
                {
                    "command": "qbs.clean",
                    "when": "view == qbs-project && qbs:extension-activated",
                    "group": "1_outline"
                },
                {
                    "command": "qbs.resolveWithForceProbesExecution",
                    "when": "view == qbs-project && qbs:extension-activated",
                    "group": "1_outline"
                },
                {
                    "command": "qbs.rebuild",
                    "when": "view == qbs-project && qbs:extension-activated",
                    "group": "1_outline"
                }
            ],
            "view/item/context": [
                {
                    "command": "qbs.cleanProduct",
                    "when": "viewItem == product-node",
                    "group": "product_context@1"
                },
                {
                    "command": "qbs.buildProduct",
                    "when": "viewItem == product-node",
                    "group": "product_context@2"
                },
                {
                    "command": "qbs.cleanSubProject",
                    "when": "viewItem == sub-project-node",
                    "group": "project_context@1"
                },
                {
                    "command": "qbs.buildSubProject",
                    "when": "viewItem == sub-project-node",
                    "group": "project_context@2"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "QBS configuration",
            "properties": {
                "qbs.qbsPath": {
                    "type": "string",
                    "default": "qbs",
                    "description": "%qbs-tools.configuration.qbs.qbsPath.description%",
                    "scope": "resource"
                },
                "qbs.settingsDirectory": {
                    "type": "string",
                    "default": "",
                    "description": "%qbs-tools.configuration.qbs.settingsDirectory.description%",
                    "scope": "resource"
                },
                "qbs.buildDirectory": {
                    "type": "string",
                    "default": "${sourceDirectory}/build/${profileName}_${configurationName}",
                    "description": "%qbs-tools.configuration.qbs.buildDirectory.description%",
                    "scope": "resource"
                },
                "qbs.commandEchoMode": {
                    "type": "string",
                    "default": "summary",
                    "enum": [
                        "command-line",
                        "command-line-with-environment",
                        "silent",
                        "summary"
                    ],
                    "description": "%qbs-tools.configuration.qbs.commandEchoMode.description%",
                    "scope": "resource"
                },
                "qbs.maxBuildJobs": {
                    "type": "number",
                    "default": 0,
                    "description": "%qbs-tools.configuration.qbs.maxBuildJobs.description%",
                    "scope": "resource"
                },
                "qbs.keepGoing": {
                    "type": "boolean",
                    "default": false,
                    "description": "%qbs-tools.configuration.qbs.keepGoing.description%",
                    "scope": "resource"
                },
                "qbs.forceProbes": {
                    "type": "boolean",
                    "default": false,
                    "description": "%qbs-tools.configuration.qbs.forceProbes.description%",
                    "scope": "resource"
                },
                "qbs.cleanInstallRoot": {
                    "type": "boolean",
                    "default": false,
                    "description": "%qbs-tools.configuration.qbs.cleanInstallRoot.description%",
                    "scope": "resource"
                },
                "qbs.errorHandlingMode": {
                    "type": "string",
                    "default": "relaxed",
                    "enum": [
                        "relaxed",
                        "strict"
                    ],
                    "description": "%qbs-tools.configuration.qbs.errorHandlingMode.description%",
                    "scope": "resource"
                },
                "qbs.logLevel": {
                    "type": "string",
                    "default": "info",
                    "enum": [
                        "error",
                        "warning",
                        "info",
                        "debug"
                    ],
                    "description": "%qbs-tools.configuration.qbs.logLevel.description%",
                    "scope": "resource"
                },
                "qbs.launchFilePath": {
                    "type": "string",
                    "default": "${sourceDirectory}/.vscode/launch.json",
                    "description": "%qbs-tools.configuration.qbs.launchFilePath.description%",
                    "scope": "resource"
                },
                "qbs.showDisabledProjectItems": {
                    "type": "boolean",
                    "default": true,
                    "description": "%qbs-tools.configuration.qbs.showDisabledProjectItems.description%",
                    "scope": "resource"
                },
                "qbs.configurationsFilePath": {
                    "type": "string",
                    "default": "${sourceDirectory}/.vscode/qbs-configurations.json",
                    "description": "%qbs-tools.configuration.qbs.configurationsFilePath.description%",
                    "scope": "resource"
                },
                "qbs.clearOutputBeforeOperation": {
                    "type": "boolean",
                    "default": false,
                    "description": "%qbs-tools.configuration.qbs.clearOutputBeforeOperation.description%",
                    "scope": "resource"
                },
                "qbs.saveBeforeBuild": {
                    "type": "boolean",
                    "default": true,
                    "description": "%qbs-tools.configuration.qbs.saveBeforeBuild.description%",
                    "scope": "resource"
                },
                "qbs.autoResolve": {
                    "type": "boolean",
                    "default": true,
                    "description": "%qbs-tools.configuration.qbs.autoResolve.description%",
                    "scope": "resource"
                },
                "qbs.buildBeforeRun": {
                    "type": "boolean",
                    "default": true,
                    "description": "%qbs-tools.configuration.qbs.buildBeforeRun.description%",
                    "scope": "resource"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "qbs-sidebar",
                    "title": "QBS",
                    "icon": "res/qbs-view-icon.svg"
                }
            ]
        },
        "views": {
            "qbs-sidebar": [
                {
                    "id": "qbs-project",
                    "name": "%qbs-tools.configuration.views.qbs.project.description%"
                }
            ]
        }
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "clean": "gulp clean",
        "package": "gulp package"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.73.1",
        "@types/which": "~1.3.1",
        "del": "^4.1.1",
        "glob": "^7.1.6",
        "gulp-filter": "^5.1.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-typescript": "^5.0.1",
        "gulp": "^4.0.2",
        "mocha": "^8.1.3",
        "run-sequence": "^2.2.1",
        "typescript": "^4.0.2",
        "vsce": "^1.79.5",
        "vscode-jsonrpc": "3.5.0",
        "vscode-nls-dev": "^3.3.2"
    },
    "dependencies": {
        "chokidar": "^3.4.3",
        "jsonc-parser": "^3.0.0",
        "vscode-cpptools": "^4.0.1",
        "vscode-nls": "^4.1.1",
        "which": "~1.3.0"
    },
    "overrides": {
        "graceful-fs": "^4.2.10"
    }
}
